
<%- include('partials/header'); -%>
<div class="container-fluid">
    <div class="card-header">
        <h3 class="text-dark mb-4">Appointment Details</h3>
    </div>
    <div class="row mb-3">
        <div class="row">
            <div class="col">
                    <div class="card-body">
                        <div class="form-row">
                            <div class="col">
                                <div class="form-group"><label><strong>Doctor</strong></label>
                                    <input class="form-control" type="text" id="dname" name="dname" readonly></div>
                            </div>
                            <div class="col">
                                <div class="form-group"><label><strong>Patient</strong></label>
                                    <input class="form-control" type="text" id="pname" name="pname" readonly></div>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="col">
                                <div class="form-group"><label><strong>Date</strong></label>
                                    <input class="form-control" type="text" name="Username" readonly></div>
                            </div>
                            <div class="col">
                                <div class="form-group"><label for="time"><strong>Time</strong></label>
                                    <input class="form-control" type="text" readonly></div>
                            </div>
                        </div>

                    </div>
                <button class="btn btn-danger btn-block text-white btn-user" type="submit">Cancel Appointment</button>
                </div>
            </div>
        </div>
    </div>
</div>

</div>
<%- include('partials/footer'); -%>
